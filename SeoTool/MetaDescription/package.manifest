{
  // we can define multiple editors
  "$schema": "http://json.schemastore.org/package.manifest",
  "propertyEditors": [
    {
      /*this must be a unique alias*/
      "alias": "MetaDescription",
      /*the name*/
      "name": "Meta Description",
      /*the icon*/
      "icon": "icon-code",
      /*grouping for "Select editor" dialog*/
      "group": "Media",
      /*the HTML file we will load for the editor*/
      "editor": {
        "view": "~/App_Plugins/SeoTool/MetaDescription/MetaDescription.html"
      }
    }
  ],
  // array of files we want to inject into the application on app_start
  "javascript": [
    "~/App_Plugins/SeoTool/MetaDescription/js/MetaDescription.controller.js"
  ],

  "css": [
    "~/App_Plugins/SeoTool/MetaDescription/css/MetaDescription_styles.css"
  ]
}